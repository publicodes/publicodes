chiffre d'affaires:
  unité: €
auto-entrepreneur:
  par défaut: non

date: 01/2025

cotisations:
  public: oui
  valeur:
    taux * chiffre d'affaires
  avec:
    taux:
      variations:
        - si: date >= 01/2025
          alors: 20%
        - sinon: 15%

charges:
  applicable si:  auto-entrepreneur
  par défaut:
    valeur: 10% * chiffre d'affaires
    plancher: 100€


revenu net:
  public: oui
  somme:
    - chiffre d'affaires
    - (- cotisations)
    - (- charges)

exemples:
exemples . CA élevé:
  public: oui
  valeur: revenu net
  contexte:
    chiffre d'affaires: 100000€
    charges: 8000€
