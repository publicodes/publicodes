chiffre d'affaires:
  par défaut: 
    valeur: 1000
    inversion avec: revenu net

auto-entrepreneur: 
  par défaut: non

date:
  par défaut: 01/2025

cotisations:
  valeur:
    taux * chiffre d'affaires
  avec:
    taux: 
      variations: 
        - si: date >= 01/2025 
          alors: 20%
        - sinon: 15%

charges:
  applicable si:  auto-entrepreneur

  
  par défaut: 
    10% * chiffre d'affaires 

charges . x: oui

revenu net:
  public: oui
  par défaut: 
    somme:
      - chiffre d'affaires
      - (- cotisations)
      - (- charges)

toto: 
  public: oui
  valeur: revenu net