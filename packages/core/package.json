{
	"name": "publicodes",
	"version": "1.6.0",
	"description": "A declarative language for encoding public algorithm",
	"types": "dist/index.d.ts",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/publicodes/publicodes.git",
		"directory": "packages/core"
	},
	"bugs": "https://github.com/publicodes/publicodes/issues",
	"homepage": "https://publi.codes/",
	"license": "MIT",
	"files": [
		"dist/"
	],
	"devDependencies": {
		"@types/chai": "^4.3.6",
		"@types/eslint-plugin-mocha": "^10",
		"@types/mocha": "^9.1.1",
		"@types/moo": "^0",
		"@types/nearley": "^2",
		"@types/sinon-chai": "^3.2.10",
		"chai": "^4.3.8",
		"dedent-js": "1.0.1",
		"eslint-plugin-mocha": "^10.5.0",
		"intl": "^1.2.5",
		"mitata": "^0.1.6",
		"mocha": "^9.2.2",
		"modele-social": "^3.0.0",
		"sinon": "^12.0.1",
		"sinon-chai": "^3.7.0",
		"ts-node": "^10.9.1",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3",
		"yaml": "^2.6.0"
	},
	"scripts": {
		"codegen": "nearleyc ./src/grammar.ne -o ./src/grammar.codegen.js",
		"build": "tsup",
		"dev": "tsup --watch",
		"test:type": "tsc --noEmit",
		"test": "mocha \"./test/**/*.test.{js,ts}\"",
		"bench": "tsup --entry.bench ./bench/index.ts && node ./dist/bench.js"
	},
	"dependencies": {
		"moo": "^0.5.2",
		"nearley": "^2.20.1"
	}
}
